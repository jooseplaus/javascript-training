// Funktsioon, mis tervitab kasutajat
function greetUser(name) {
    // Saame hetkelise kuup채eva
    const today = new Date();
    
    // Kuup채eva formaat: P채ev-Kuu-Aasta
    const dateString = today.toLocaleDateString();

    // Tervitame kasutajat
    return `Tere, ${name}! T채na on ${dateString}.`;
}

// Funktsiooni testimine
console.log(greetUser("Joosep"));
